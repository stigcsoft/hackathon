<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>backend_Panel</servlet-name>
        <servlet-class>backend.backend_Panel</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>backend.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Summary</servlet-name>
        <servlet-class>backend.Summary</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>savingHistory</servlet-name>
        <servlet-class>backend.savingHistory</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>usageHistory</servlet-name>
        <servlet-class>backend.usageHistory</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>transferHistory</servlet-name>
        <servlet-class>backend.transferHistory</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>backend_Panel</servlet-name>
        <url-pattern>/backend_Panel</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Summary</servlet-name>
        <url-pattern>/Summary</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>savingHistory</servlet-name>
        <url-pattern>/savingHistory</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>usageHistory</servlet-name>
        <url-pattern>/usageHistory</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>transferHistory</servlet-name>
        <url-pattern>/transferHistory</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
